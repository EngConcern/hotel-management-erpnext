 "font_size": 14,
 "html": "<!-- Font Awesome & Roboto Font -->\n<link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css\">\n<style>\n@import url('https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700&display=swap');\n\nbody, table {\n    font-family: 'Roboto', sans-serif;\n    font-size: 12px;\n    margin: 0;\n    padding: 0;\n}\n\ntable {\n    width: 100%;\n    border-collapse: collapse;\n}\n\nthead {\n    display: table-header-group;\n}\n\ntfoot {\n    display: table-footer-group;\n}\n\ntr, td, th {\n    page-break-inside: avoid;\n}\n\nhr {\n    margin-block-start: 0;\n    margin-block-end: 0.1em;\n}\n\n/* Layout */\n.main {\n    margin: 10px;\n}\n\n/* Typography */\n.text-uppercase {\n    text-transform: uppercase;\n}\n\n.font-weight-bold {\n    font-weight: bold;\n}\n\n.font-small {\n    font-size: 10px;\n}\n\n.text-align-center {\n    text-align: center;\n}\n\n.righth p, .righth h1 {\n    text-align: right;\n}\n\n/* Header Box Styling */\n.headb td {\n    border-radius: 10px;\n}\n\n/* Items Table */\n.items-table th, .items-table td {\n    font-size: 12px;\n    padding: 6px;\n    border: 1px solid #000;\n}\n\n.items-table thead th {\n    background: #f5f5f5;\n}\n\n/* Banking & Totals Section */\n.banking-details {\n    margin-top: 20px;\n    display: flex;\n    justify-content: space-between;\n    gap: 20px;\n}\n\n.banking-left {\n    width: 50%;\n}\n\n.banking-right {\n    width: 50%;\n}\n\n.banking-right table {\n    width: 100%;\n    font-size: 12px;\n}\n\n.banking-right td {\n    padding: 4px;\n}\n\n/* Footer */\n.footer- {\n    background-color: black;\n    width: 100%;\n    position: fixed;\n    bottom: 0;\n    text-align: center;\n    height: 20px;\n}\n\n/* Print Styles */\n@media print {\n    body {\n        margin: 0;\n    }\n\n    .main {\n        page-break-inside: avoid;\n    }\n\n    .items-table, .items-table thead, .items-table tbody, .items-table tr, .items-table td, .items-table th {\n        page-break-inside: avoid;\n    }\n\n    .banking-details {\n        flex-direction: column;\n    }\n\n    .banking-left, .banking-right {\n        width: 100%;\n    }\n\n    .footer- {\n        position: fixed;\n        bottom: 0;\n    }\n}\n</style>\n\n<!-- Invoice HTML -->\n<table>\n    <tbody>\n        <!-- Header Section -->\n        <tr>\n            <td>\n                <div class=\"main\">\n                    <table class=\"headb\">\n                        <tr style=\"border: 1px solid black;\">\n                            <!-- Logo -->\n                            <td style=\"width: 35%;\">\n                                <img src=\"/files/IMG-20250424-WA0092.jpg\" alt=\"Logo\" width=\"100%\" style=\"margin-top: 22px;\">\n                            </td>\n                            <!-- Title -->\n                            <td style=\"width: 30%; padding: 10px;\">\n                                <div class=\"text-align-center\">\n                                    <h1 style=\"margin: 0; text-indent: 40px;\">Sales Invoice</h1>\n                                </div>\n                            </td>\n                            <!-- Company Info -->\n                            <td style=\"width: 35%;\">\n                                {% if doc.doctype == \"Sales Invoice\" or doc.doctype == \"Quotation\" or doc.doctype == \"Booking\" %}\n                                <div class=\"righth\">\n                                    <p>{{ frappe.db.get_value(\"Company\", doc.company, \"custom_address_1\") or \"\" }}</p>\n                                    <p>{{ frappe.db.get_value(\"Company\", doc.company, \"custom_address_2\") or \"\" }}</p>\n                                    <p>{{ frappe.db.get_value(\"Company\", doc.company, \"custom_address_3\") or \"\" }}</p>\n                                    <p>{{ frappe.db.get_value(\"Company\", doc.company, \"phone_no\") or \"\" }}</p>\n                                    <p>{{ frappe.db.get_value(\"Company\", doc.company, \"custom_phone_2\") or \"\" }}</p>\n                                    <p>{{ frappe.db.get_value(\"Company\", doc.company, \"email\") or \"\" }}</p>\n                                    <p>{{ frappe.db.get_value(\"Company\", doc.company, \"custom_email_2\") or \"\" }}</p>\n                                </div>\n                                {% endif %}\n                            </td>\n                        </tr>\n                    </table>\n                </div>\n\n                <!-- Customer & Invoice Details -->\n                <div class=\"main\">\n                    <table style=\"border: 1px solid black; margin-top: 10px;\">\n                        <tr>\n                            <!-- Customer Info -->\n                            <td style=\"width: 60%; padding: 5px;\">\n                                <p><strong>To:</strong> {{ doc.customer_name }}</p>\n                                <p><strong>Address:</strong> {{ doc.get_formatted(\"custom_address\") }}</p>\n                                <p><strong>Customer VAT:</strong> {{ doc.get_formatted(\"custom_customer_vat\") }}</p>\n                                <p><strong>Customer TIN:</strong> {{ doc.get_formatted(\"custom_customer_tin\") }}</p>\n                                <p><strong>Time:</strong> {{ doc.get_formatted(\"posting_date\") }}</p>\n                                <p><strong>Reference:</strong> {{ doc.get_formatted(\"custom_sales_reference\") }}</p>\n                            </td>\n\n                            <!-- Invoice Info -->\n                            <td style=\"width: 40%; padding: 5px;\">\n                                <table>\n                                    <tr><td class=\"font-weight-bold\">Salesman:</td><td>Tinotenda M</td></tr>\n                                    <tr><td class=\"font-weight-bold\">Doc No:</td><td class=\"text-uppercase font-weight-bold\">{{ doc.name }}</td></tr>\n                                    <tr><td class=\"font-weight-bold\">Invoice Date:</td><td>{{ doc.get_formatted(\"posting_date\") }}</td></tr>\n                                    <tr><td class=\"font-weight-bold\">Payment Terms:</td><td></td></tr>\n                                    <tr><td class=\"font-weight-bold\">VAT #:</td><td>2003467711</td></tr>\n                                    <tr><td class=\"font-weight-bold\">TIN #:</td><td>2000899443</td></tr>\n                                </table>\n                            </td>\n                        </tr>\n                    </table>\n                </div>\n                <hr>\n            </td>\n        </tr>\n\n        <!-- Items Section -->\n        <tr>\n            <td>\n                <div class=\"main\">\n                    <div class=\"body-table\">\n                        <table class=\"items-table\">\n                            <thead>\n                                <tr>\n                                    <th style=\"width: 7%;\">#</th>\n                                    <th style=\"width: 60%;\">Item & Description</th>\n                                    <th style=\"width: 11%;\">Qty</th>\n                                    <th style=\"width: 10%;\">Price (Inc)</th>\n                                    <th style=\"width: 10%;\">VAT</th>\n                                    <th style=\"width: 11%;\">Sum (Inc)</th>\n                                </tr>\n                            </thead>\n                            <tbody class=\"items-body\">\n                                {% for row in doc.items %}\n                                <tr>\n                                    <td>{{ loop.index }}</td>\n                                    <td>{{ row.item_name }}</td>\n                                    <td>{{ row.qty }}</td>\n                                    <td>{{ row.rate }}</td>\n                                    <td>{{ \"{:.2f}\".format((row.amount - row.net_amount) | round(2)) }}</td>\n                                    <td>{{ \"{:.2f}\".format(row.amount | round(2)) }}</td>\n                                </tr>\n                                {% endfor %}\n                            </tbody>\n                        </table>\n                    </div>\n                </div>\n            </td>\n        </tr>\n\n        <!-- Banking & Totals -->\n        <tr>\n            <td>\n                <div class=\"main banking-details\">\n                    <div class=\"banking-left\">\n                        <p><strong style=\"font-size: 15px;\">Banking Details:</strong></p>\n                        <p class=\"font-small\"><strong>Bank Name:</strong> CABS</p>\n                        <p class=\"font-small\"><strong>Account Name:</strong> DUNEXAS INVESTMENTS</p>\n                        <p class=\"font-small\"><strong>Account Number:</strong> 1144292441</p>\n                    </div>\n                    <div class=\"banking-right\">\n                        <table>\n                            <tr><td class=\"font-weight-bold\">TOTAL TAX:</td><td>{{ doc.get_formatted(\"total_taxes_and_charges\") }}</td></tr>\n                            <tr><td class=\"font-weight-bold\">TOTAL INVOICE:</td><td>{{ doc.get_formatted(\"grand_total\") }}</td></tr>\n                            <tr><td class=\"font-weight-bold\">TOTAL GOODS:</td><td>{{ doc.get_formatted(\"total_qty\") }}</td></tr>\n                            <tr><td class=\"font-weight-bold\">CUSTOMER BALANCE:</td\n                            <td>{{ doc.get_formatted(\"grand_total\") }}</td></tr>\n                        </table>\n                                        </div>\n            </div>\n        </td>\n    </tr>\n\n    <!-- Footer Section -->\n    <tr>\n        <td>\n            <div class=\"footer-\">\n                <p style=\"color: #fff; font-size: 10px;\">\u00a9 Havano. All rights reserved.</p>\n            </div>\n        </td>\n    </tr>\n</tbody>\n</table>\n\n",
